<h1>Rijksmuseum Art Online</h1>

<!-- (1)[mobile][desktop: search section] -->
<section class='mainContent'>
	<article class='artItemContainer'>
		<a href='/art'>< back to all results</a>
		<div>
			<img
				srcset='{{sliceUrl artData.webImage.url}}=s500 500w, {{sliceUrl
					artData.webImage.url
				}}=s750 750w, {{sliceUrl artData.webImage.url}}=s1000 1000w'
				sizes='(max-width: 600px) 500px, (max-width: 1000px) 750px, 1000px'
				src='{{sliceUrl artData.webImage.url}}=s750'
				alt='{{artData.longTitle}}'
			/>
			<div>
				<h2>{{artData.title}}</h2>
				<p>{{artData.principalOrFirstMaker}}</p>
				<p>{{artData.subTitle}}</p>
				<p>{{artData.plaqueDescriptionEnglish}}</p>
			</div>
		</div>
	</article>
</section>